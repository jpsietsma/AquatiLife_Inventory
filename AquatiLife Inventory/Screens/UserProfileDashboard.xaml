<Window x:Class="AquatiLife_Inventory.Screens.UserProfileDashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AquatiLife_Inventory.Screens"
        mc:Ignorable="d"
        Title="UserProfileDashboard" Height="450" Width="800">
    <Grid>
        <TextBlock Text=" UserName: " Padding="200,0,0,0" />
        <TextBlock Text="{ Binding UserName }" Padding="275, 0, 0, 0" />
        <TextBlock Text="Permission: " Padding="200,25,0,0" />
        <TextBlock Text="{ Binding PermissionLevels }" Padding="275, 25, 0, 0" />
    </Grid>
</Window>
